#include<stdio.h>
int main()
{
	int i ,j , m, n;
	printf("enter the 'm'& 'n' values");
	scanf("%d%d",&m,&n);
	int a[m][n],b[m][n],c[m][n];
	printf("enter the 'A' array");
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
		scanf("%d",&a[i][j]);	
		}
	}
	printf("enter the 'B' array");
for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
		scanf("%d",&b[i][j]);	
		}
	}
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
		c[i][j]=a[i][j]*b[i][j];
		}
	}
	printf("the result\n");
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
	     	printf("%3d",c[i][j]);
		}
			printf("\n");
	}
}
